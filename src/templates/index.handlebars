{{> header}}
	<div id = "ajax-wrapper">


		<div id = "index">

			<div class = "homepage-hero full-browser-height content-wrapper scroll-reveal">

				<div class = "vertical-center">
					<h1 class = "mblack-black"><span class = "subhead">{{intro.smallCopy}}<span class = "h-line"></span></span> {{intro.bigCopy}}</h1>
				</div>

				<video playsinline autoplay loop id="video-background" muted poster="  {{intro.videoBackground.poster}}  " preload = "none">
				  <source src="{{intro.videoBackground.url}}" type="video/mp4">
				</video>

			</div>





			<div class = "core-disciplines full-browser-height content-wrapper scroll-reveal">

				<div class = "vertical-center">
					<h3 class = "preg-yellow"><span class = "subhead">{{core-disciplines.smallCopy}}<span class = "h-line"></span></span> {{core-disciplines.bigCopy}}</h3>
					<a href = "{{core-disciplines.button.url}}" data-type="page-transition">{{core-disciplines.button.text}}<span class = "yellow"></span><span class = "black"></span></a>
					<div class = "eye">
						<img title = "blinking eyeball" description = "blinking eyeball" class = "absolute-center" id = "eye-anim" src="../../build/img/home/eye.gif " alt="eye" width="542" height="310" />
						<img title = "maine island" description = "maine island" id = "island" src="../../build/img/home/island.jpg" alt="eye" width="542" height="310" />
					</div>
				</div>
			</div>





			<div itemscope itemtype="http://schema.org/VideoObject" class = "brand-reel video-wrapper scroll-reveal">

				<meta itemprop="name" content="2017 Brand Reel"/>
				<meta itemprop="contentURL" content="{{brand-reel.videoUrl}}"/>
				<meta itemprop="description" content="Kingspoke 2017 Brand Reel"/>
				<meta itemprop="thumbnailURL" content="{{brand-reel.videoPoster}}"/>
				<meta itemprop="duration" content="T2M12S" />
				<meta itemprop="uploadDate" content="2018-01-05T08:00:00+08:00" />

				<div class = "video-controls" data-id = "brand-reel-video">
					<button type="button" class="play-pause play play-button absolute-center"></button>
					<div class ="scrubber-bar"><span></span></div>
					<button type="button" class="full-screen"></button>
				</div>

				<video class = "paused" data-id = "brand-reel-video" poster="{{brand-reel.videoPoster}}" preload = "none" >
				    <source src=" {{brand-reel.videoUrl}} " type='video/mp4; codecs="avc1.42E01E, mp4a.40.2"'>
				</video>

			</div>
		</div>

	</div><!-- ajax wrapper -->
{{> footer}}
